{
  "name": "sqs-consumer-example-express-simple",
  "version": "1.0.0",
  "description": "An example app for SQS Consumer",
  "main": "./dist/app.js",
  "scripts": {
    "clean": "rm -fr dist/*",
    "build": "npm run clean && tsc",
    "dev": "NODE_ENV=development ts-node ./src/app.ts",
    "start": "npm run docker:up",
    "start:node": "node ./dist/app.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^18.11.15",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "3.226.0",
    "express": "4.18.2",
    "sqs-consumer": "6.0.2",
    "sqs-producer": "3.0.1"
  }
}
